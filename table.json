{
  "/account": {
    "get": "account:list",
    "post": "account:add"
  },
  "/account/[0-9]+": {
    "get": "account:view",
    "put": "account:update",
    "delete": "account:delete"
  },
  "/account/[0-9]+/password": {
    "patch": "account:password"
  },
  "/account/[0-9]+/password/self": {
    "patch": "account:password_self"
  },
  "/account/[0-9]+/active": {
    "patch": "account:active"
  },
  "/account/[0-9]+/inactive": {
    "patch": "account:inactive"
  },

