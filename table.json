    "get": "asset:view",
    "put": "asset:update",
    "delete": "asset:delete"
  },

  "/asset/department": {
    "get": "asset_department:list",
    "post": "asset_department:add"
  },
  "/asset/department/[0-9]+": {
    "put": "asset_department:update",
    "delete": "asset_department:delete"
  },

  "/asset/group": {
    "get": "asset_group:list",
    "post": "asset_group:add"
  },
