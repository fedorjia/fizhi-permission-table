  "/check": {
    "get": "check:list",
    "post": "check:create"
  },
  "/check/history": {
    "get": "check:list_history"
  },
  "/check/asset/[0-9]+/manual": {
    "post": "check:manual"
  },
  "/check/asset/[0-9]+/repair": {
    "post": "check:repair_request",
    "put": "check:repair_done"
  },
  "/check/asset/[0-9]+/scrap": {
    "post": "check:scrap_request",
    "put": "check:scrap_done"
  },

